{
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-console": "warn",
    "sort-imports": ["error", { "ignoreCase": true, "ignoreDeclarationSort": true }],
    "no-unused-vars": "warn"
  }
}
